@prefix : <http://scripto.org/vocab#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

:content a rdf:Property ;
    rdfs:label "content"@en ;
    rdfs:comment "A plain text representation of the content of a resource."@en .

:transcription a rdf:Property ;
    rdfs:label "transcription"@en ;
    rdfs:comment "A transcription of a resource."@en ;
    rdfs:subPropertyOf :content .

:translation a rdf:Property ;
    rdfs:label "translation"@en ;
    rdfs:comment "A translation of a resource."@en ;
    rdfs:subPropertyOf :content .
